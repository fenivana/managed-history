{
  "name": "spa-history",
  "version": "0.7.0",
  "description": "A HTML5 history routing library for single-page application.",
  "main": "dist/spaHistory",
  "scripts": {
    "build": "rollup -c rollup.config.esm.mjs && rollup -c rollup.config.umd.mjs",
    "example": "ws --spa example.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangfengming/spa-history.git"
  },
  "keywords": [
    "history",
    "pushState",
    "replaceState"
  ],
  "author": "Jiang Fengming <jiangfengming87@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangfengming/spa-history/issues"
  },
  "homepage": "https://github.com/jiangfengming/spa-history#readme",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-enough": "^0.3.4",
    "local-web-server": "^2.6.1",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
