{
  "name": "spa-history",
  "version": "0.6.0",
  "description": "A HTML5 history routing library for single-page application.",
  "main": "dist/spaHistory.umd.js",
  "module": "dist/spaHistory.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/spa-history.git"
  },
  "keywords": [
    "history"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/spa-history/issues"
  },
  "homepage": "https://github.com/fenivana/spa-history#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-enough": "^0.3.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
