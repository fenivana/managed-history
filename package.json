{
  "name": "spa-history",
  "version": "0.3.1",
  "description": "A rewritable and stateful browser history.",
  "main": "dist/SpaHistory.js",
  "scripts": {
    "dev": "rollup -c rollup.bundle.config.js -w",
    "build": "rollup -c && rollup -c rollup.bundle.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/spa-history.git"
  },
  "keywords": [
    "history"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/spa-history/issues"
  },
  "homepage": "https://github.com/fenivana/spa-history#readme",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-enough": "0.2.6",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-watch": "3.2.2"
  },
  "dependencies": {
    "browser-url": "1.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
