{
  "name": "spa-history",
  "version": "0.3.1",
  "description": "A rewritable and stateful browser history.",
  "main": "dist/SpaHistory.js",
  "scripts": {
    "dev": "rollup src/index.js -f umd -o dist/SpaHistory.js",
    "build": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/spa-history.git"
  },
  "keywords": [
    "history"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/spa-history/issues"
  },
  "homepage": "https://github.com/fenivana/spa-history#readme",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "eslint": "3.19.0",
    "eslint-config-enough": "0.2.6",
    "rollup": "0.41.6"
  },
  "dependencies": {
    "browser-url": "1.3.2"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "loose": true
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
