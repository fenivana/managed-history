{
  "name": "spa-history",
  "version": "0.4.1",
  "description": "A HTML5 history routing library for single-page application.",
  "main": "",
  "scripts": {
    "build": "rollup -c rollup.config.HashHistory.js && rollup -c rollup.config.PathHistory.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/spa-history.git"
  },
  "keywords": [
    "history"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/spa-history/issues"
  },
  "homepage": "https://github.com/fenivana/spa-history#readme",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-enough": "0.2.6",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1"
  },
  "dependencies": {
    "browser-url": "1.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
